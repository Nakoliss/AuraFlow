{
  "name": "@aura-flow/mobile",
  "version": "1.0.0",
  "private": true,
  "description": "AuraFlow mobile app built with Expo React Native",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "tsc",
    "dev": "expo start --dev-client",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aura-flow/common": "workspace:*",
    "@aura-flow/ui": "workspace:*",
    "expo": "~49.0.0",
    "expo-router": "^2.0.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "typescript": "^5.1.3",
    "vitest": "^0.34.0"
  }
}